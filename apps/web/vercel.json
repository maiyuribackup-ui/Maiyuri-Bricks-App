{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "npm config delete //registry.npmjs.org/:_authToken 2>/dev/null; npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "crons": [
    {
      "path": "/api/odoo/cron",
      "schedule": "0 0 * * *"
    }
  ]
}
